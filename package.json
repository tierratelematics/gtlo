    "typedoc": "typedoc --out doc --theme minimal --excludeNotExported --mode file src"
  },
  "devDependencies": {
    "@types/expect.js": "0.3.29",
    "@types/mocha": "5.2.7",
    "@types/node": "10.14.15",
    "@types/sinon": "7.5.0",
    "expect.js": "0.3.1",
    "mocha": "6.2.2",
    "rimraf": "3.0.0",
    "sinon": "7.5.0",
    "ts-node": "8.4.1",
    "tslint": "5.20.1",
    "typemoq": "2.1.0",
    "typescript": "3.7.2"
  }
}
